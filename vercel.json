{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/:pathMatch(.*)*",
      "dest": "index.html"
    }
  ]
}